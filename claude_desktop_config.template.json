{
  "mcpServers": {
     "earthly_frames": {
      "command": "uv",
      "version": "0.1.1",
      "args": [
        "--directory",
        "/Volumes/LucidNonsense/White",
        "run",
        "app/reference/mcp/earthly_frames/main.py"
      ]
    },
    "earthly_frames_discogs": {
      "command": "uv",
      "args": [
        "--directory",
        "/Volumes/LucidNonsense/White",
        "run",
        "app/reference/mcp/discogs/main.py"
      ]
    },
    "lucid_nonsense_access": {
        "command": "uv",
        "version": "2.0",
        "args": [
            "--directory",
            "/Volumes/LucidNonsense/White",
            "run",
            "app/reference/mcp/lucid_nonsense_access/main.py"
        ]
    },
    "midi_mate": {
        "command": "uv",
        "args": [
            "--directory",
            "/Volumes/LucidNonsense/White",
            "run",
            "app/reference/mcp/midi_mate/main.py"
        ]
    },
    "earthly-frames-todoist": {
      "command": "uv",
      "args": [
        "--directory",
        "/Volumes/LucidNonsense/White",
        "run",
        "python",
        "app/reference/mcp/todoist/main.py"
      ],
      "env": {
        "TODOIST_API_TOKEN": "YOUR_TODOIST_API_TOKEN_HERE"
      }
    },
    "orange-mythos": {
      "command": "uv",
      "args": [
        "--directory",
        "/Volumes/LucidNonsense/White",
        "run",
        "python",
        "app/reference/mcp/rows_bud/orange_mythos_server.py"
      ],
      "env": {
        "ANTHROPIC_API_KEY": "YOUR_ANTHROPIC_API_KEY_HERE",
        "ORANGE_CORPUS_DIR": "/Volumes/LucidNonsense/White/app/reference/mcp/rows_bud"
      }
    }
  }
}

